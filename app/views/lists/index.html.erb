<h1> <%= link_to "Create a movie list", new_list_path, class: "btn btn-primary" %> </h1>

<ul>
  <% @lists.each do |list| %>
    <li> <%= link_to "#{list.name}" , list_path(list) %> |||||| <%= link_to "delete this list", list_path(list), class: "btn btn-warning", data: { turbo_method: :delete, turbo_confirm: "Are you sure you want to delete this list?" } %></li>
  <% end %>
</ul>
